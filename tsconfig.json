{
  "compilerOptions": {
    "outFile": "./dist/nix-comment-updater.js",
    "jsx": "react",
    "strict": true,
    "jsxFactory": "createElement",
    "module": "commonjs",
    "target": "esnext",
    "sourceMap": true,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "/*": [
        "src/*"
      ],
      "u-spawn/*": [
        "src/tsmono/u-spawn/*"
      ],
      "u-spawn": [
        "src/tsmono/u-spawn"
      ],
      "u-lazy/*": [
        "src/tsmono/u-lazy/*"
      ],
      "u-lazy": [
        "src/tsmono/u-lazy"
      ],
      "u-scalar-limiter/*": [
        "src/tsmono/u-scalar-limiter/*"
      ],
      "u-scalar-limiter": [
        "src/tsmono/u-scalar-limiter"
      ],
      "u-debug/*": [
        "src/tsmono/u-debug/*"
      ],
      "u-debug": [
        "src/tsmono/u-debug"
      ],
      "u-select-version/*": [
        "src/tsmono/u-select-version/*"
      ],
      "u-select-version": [
        "src/tsmono/u-select-version"
      ],
      "u-nix/*": [
        "src/tsmono/u-nix/*"
      ],
      "u-nix": [
        "src/tsmono/u-nix"
      ],
      "u-testrunner/*": [
        "src/tsmono/u-testrunner/*"
      ],
      "u-testrunner": [
        "src/tsmono/u-testrunner"
      ],
      "u-testrunner-node-chalk/*": [
        "src/tsmono/u-testrunner-node-chalk/*"
      ],
      "u-testrunner-node-chalk": [
        "src/tsmono/u-testrunner-node-chalk"
      ],
      "u-exception-to-string/*": [
        "src/tsmono/u-exception-to-string/*"
      ],
      "u-exception-to-string": [
        "src/tsmono/u-exception-to-string"
      ],
      "ts-nix-comment-updater": [
        "src/*"
      ]
    },
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "lib": [
      "es5",
      "es6",
      "dom",
      "es2015.promise",
      "es2015.collection",
      "es2015.iterable",
      "es2019",
      "dom",
      "dom.iterable"
    ],
    "baseUrl": "."
  },
  "exclude": [
    "./dist",
    "./dist/nix-comment-updater.js"
  ]
}